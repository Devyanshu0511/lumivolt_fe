    <line x1="200" y1="80" x2="200" y2="320"/>
    <line x1="212.5" y1="80" x2="212.5" y2="320"/>
    <line x1="225" y1="80" x2="225" y2="320"/>
    <line x1="237.5" y1="80" x2="237.5" y2="320"/>
    <line x1="250" y1="80" x2="250" y2="320"/>
    <line x1="262.5" y1="80" x2="262.5" y2="320"/>
    <line x1="275" y1="80" x2="275" y2="320"/>
    <line x1="287.5" y1="80" x2="287.5" y2="320"/>
    <line x1="300" y1="80" x2="300" y2="320"/>
    <line x1="312.5" y1="80" x2="312.5" y2="320"/>
    <line x1="325" y1="80" x2="325" y2="320"/>
    <line x1="337.5" y1="80" x2="337.5" y2="320"/>
    <line x1="350" y1="80" x2="350" y2="320"/>
    <line x1="362.5" y1="80" x2="362.5" y2="320"/>
    <line x1="375" y1="80" x2="375" y2="320"/>
    <line x1="387.5" y1="80" x2="387.5" y2="320"/>
    <line x1="400" y1="80" x2="400" y2="320"/>
    <line x1="412.5" y1="80" x2="412.5" y2="320"/>
    <line x1="425" y1="80" x2="425" y2="320"/>
    <line x1="437.5" y1="80" x2="437.5" y2="320"/>
