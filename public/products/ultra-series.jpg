  <!-- Premium Junction Box -->
  <rect x="270" y="290" width="60" height="25" rx="5" fill="#111827" stroke="#fbbf24" stroke-width="1"/>
  <circle cx="285" cy="302.5" r="2" fill="#dc2626"/>
  <circle cx="300" cy="302.5" r="2" fill="#fbbf24"/>
  <circle cx="315" cy="302.5" r="2" fill="#dc2626"/>

  <!-- Premium Features Indicator -->
  <circle cx="520" cy="100" r="8" fill="#fbbf24" opacity="0.8"/>
  <text x="520" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" font-weight="bold" fill="#92400e">IP68</text>

  <circle cx="520" cy="120" r="8" fill="#fbbf24" opacity="0.8"/>
  <text x="520" y="125" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" font-weight="bold" fill="#92400e">MC4</text>

  <!-- Product Label -->
  <rect x="120" y="340" width="360" height="40" rx="6" fill="rgba(255,255,255,0.95)" stroke="#d97706" stroke-width="2"/>
  <text x="300" y="362" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#92400e">Lumivolt Ultra Series Premium</text>
